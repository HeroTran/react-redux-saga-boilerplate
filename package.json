{
    "name": "react-redux-saga-boilerplate",
    "version": "0.0.1",
    "private": true,
    "author": "HeroTran",
    "description": "this is my own project for react-redux-saga",
    "main": "./src/app/index.js",
    "scripts": {
        "dev": "cross-env NODE_ENV=development webpack-dev-server --mode development --color --port 3001 --open",
        "prod": "cross-env NODE_ENV=production webpack-dev-server --mode production --color  --port 3001 --open",
        "webpack:prod": "./node_modules/.bin/webpack --env=prod --progress --profile --colors",
        "build": "yarn clean && yarn webpack:prod",
        "build-dev": "cross-env NODE_ENV=development yarn build",
        "build-prod": "cross-env NODE_ENV=production yarn build",
        "format": "prettier --write \"src/**/*.{js,jsx,css}\"",
        "lint": "cross-env NODE_ENV=development eslint --cache --format=pretty --ext .js --ext .jsx .",
        "lint:fix": "eslint --fix --ext .js,.jsx .",
        "clean": "node rimraf",
        "test": "jest --coverage --coverageDirectory=output/coverage/jest",
        "test:watch": "npm test -- --watch"
    },
    "license": "MIT",
    "dependencies": {
        "@material-ui/core": "^4.10.2",
        "axios": "^0.19.2",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-redux": "^7.2.0",
        "react-router": "^5.2.0",
        "react-router-dom": "^5.2.0",
        "redux": "^4.0.5",
        "redux-saga": "^1.1.3"
    },
    "devDependencies": {
        "@babel/plugin-proposal-class-properties": "^7.10.1",
        "@babel/plugin-proposal-decorators": "^7.10.1",
        "@babel/plugin-proposal-do-expressions": "^7.10.1",
        "@babel/plugin-proposal-export-default-from": "^7.10.1",
        "@babel/plugin-proposal-export-namespace-from": "^7.10.1",
        "@babel/plugin-proposal-function-bind": "^7.10.1",
        "@babel/plugin-proposal-function-sent": "^7.10.1",
        "@babel/plugin-proposal-json-strings": "^7.10.1",
        "@babel/plugin-proposal-logical-assignment-operators": "^7.10.1",
        "@babel/plugin-proposal-nullish-coalescing-operator": "^7.10.1",
        "@babel/plugin-proposal-numeric-separator": "^7.10.1",
        "@babel/plugin-proposal-optional-chaining": "^7.10.1",
        "@babel/plugin-proposal-pipeline-operator": "^7.10.1",
        "@babel/plugin-proposal-throw-expressions": "^7.10.1",
        "@babel/plugin-syntax-dynamic-import": "^7.8.3",
        "@babel/plugin-syntax-import-meta": "^7.10.1",
        "@babel/plugin-transform-react-constant-elements": "^7.10.1",
        "@babel/plugin-transform-react-inline-elements": "^7.10.1",
        "@babel/preset-env": "^7.10.2",
        "@babel/preset-react": "^7.10.1",
        "@babel/register": "^7.10.1",
        "@hot-loader/react-dom": "^16.13.0",
        "babel-core": "^6.26.3",
        "babel-eslint": "^10.1.0",
        "babel-jest": "^26.0.1",
        "babel-loader": "^8.1.0",
        "babel-plugin-dev-expression": "^0.2.2",
        "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
        "babel-preset-es2015": "^6.24.1",
        "clean-webpack-plugin": "^3.0.0",
        "connected-react-router": "^6.8.0",
        "cross-env": "^7.0.2",
        "css-loader": "^3.5.3",
        "dotenv-webpack": "^1.8.0",
        "enzyme": "^3.11.0",
        "enzyme-adapter-react-16": "^1.15.2",
        "eslint": "^7.2.0",
        "eslint-config-airbnb": "^18.1.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-formatter-pretty": "^3.0.1",
        "eslint-import-resolver-webpack": "^0.12.1",
        "eslint-plugin-compat": "^3.7.0",
        "eslint-plugin-import": "^2.21.2",
        "eslint-plugin-jsx-a11y": "^6.2.3",
        "eslint-plugin-prettier": "^3.1.3",
        "eslint-plugin-promise": "^4.2.1",
        "eslint-plugin-react": "^7.20.0",
        "file-loader": "^6.0.0",
        "history": "^4.10.1",
        "html-loader": "^1.1.0",
        "html-webpack-plugin": "^4.3.0",
        "i18next": "^19.4.5",
        "i18next-browser-languagedetector": "^4.2.0",
        "i18next-http-backend": "^1.0.15",
        "identity-obj-proxy": "^3.0.0",
        "immutable": "^4.0.0-rc.12",
        "jest": "^26.0.1",
        "jest-cli": "^26.0.1",
        "lint-staged": "^10.2.10",
        "lodash": "^4.17.15",
        "mini-css-extract-plugin": "^0.9.0",
        "moment": "^2.29.4",
        "node-sass": "^4.14.1",
        "optimize-css-assets-webpack-plugin": "^5.0.3",
        "prettier": "^2.0.5",
        "progress-bar-webpack-plugin": "^2.1.0",
        "react-hot-loader": "^4.12.21",
        "react-i18next": "^11.5.1",
        "redux-devtools-extension": "^2.13.8",
        "redux-immutable": "^4.0.0",
        "redux-mock-store": "^1.5.4",
        "reselect": "^4.0.0",
        "rimraf": "^3.0.2",
        "sass-loader": "^8.0.2",
        "style-loader": "^1.2.1",
        "terser-webpack-plugin": "^3.0.3",
        "url-loader": "^4.1.0",
        "webpack": "^4.43.0",
        "webpack-bundle-analyzer": "^3.8.0",
        "webpack-cli": "^3.3.11",
        "webpack-dev-server": "^3.11.0",
        "yarn": "^1.22.4"
    },
    "keywords": [
        "react",
        "redux",
        "redux-saga",
        "immutable",
        "scss",
        "material",
        "webpack",
        "hot",
        "reload",
        "jest"
    ],
    "jest": {
        "verbose": true,
        "setupFiles": [
            "./internals/scripts/jestsetup.js"
        ],
        "coverageReporters": [
            "text",
            "html"
        ],
        "transform": {
            "^.+\\.(js|jsx|mjs)$": "<rootDir>/internals/scripts/jest-transforme.js"
        },
        "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(js|jsx)$",
        "moduleFileExtensions": [
            "js",
            "jsx",
            "json",
            "scss"
        ],
        "moduleDirectories": [
            "node_modules"
        ],
        "moduleNameMapper": {
            "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/internals/mocks/fileMock.js",
            "\\.(scss|less|css)$": "identity-obj-proxy"
        },
        "testURL": "http://localhost"
    },
    "lint-staged": {
        "*.{js,jsx}": [
            "cross-env NODE_ENV=development eslint --cache --format=pretty --ext .js --ext .jsx .",
            "git add"
        ],
        "{*.json,.{babelrc,eslintrc,prettierrc}}": [
            "prettier --ignore-path .eslintignore --parser json --write",
            "git add"
        ],
        "*.{yml}": [
            "prettier --ignore-path .eslintignore --single-quote --write",
            "git add"
        ]
    }
}
